<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 <hibernate-mapping>
 <class name="Insurance" table="insurance_crud">
 	<id name="id" type="int" column="id"></id>
 	<property name="policyNo" column="policy_no" type="int"/>
 	<property name="name" column="name" type="string"/>
 	<property name="tenure" column="tenure" type="int"/>
 	<property name="amount" column="amount" type="int"/>
 </class>
 </hibernate-mapping>